<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<Class targetClassName="PersonEntity" context="Save">

  <Property name="name">
    <RequiredRule/>
    <PersonUniqueNameRule/>
  </Property>

  <Property name="currentCapacity">
    <RequiredRule/>
  </Property>

  <Property name="emailAddress">
    <EmailRule/>
  </Property>

  <Property name="hasAccount">
    <PersonHasAccountEnabled/>
  </Property>

  <Property name="userName">
    <UsernameRule/>
    <PersonUniqueUsernameRule/>
  </Property>

</Class>